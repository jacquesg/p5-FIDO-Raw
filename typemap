	sv_setref_pv($arg, \"FIDO::Raw::${(my $ntt = $ntype) =~ s/_/::/g;\$ntt}\", (void *) $var);

INPUT
T_FIDO_REF
	if (sv_isobject($arg) &&
	    sv_derived_from($arg, \"FIDO::Raw::${(my $ntt = $ntype) =~ s/_/::/g;\$ntt}\")) {
		$var = INT2PTR($type, SvIV((SV *) SvRV($arg)));
	} else {
		$var = NULL;
		croak_usage(\"$var is not of type FIDO::Raw::${(my $ntt = $ntype) =~ s/_/::/g;\$ntt}\");
	}
